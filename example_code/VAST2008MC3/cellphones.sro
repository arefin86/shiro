use "data.sro"
use "shapes.sro"

graph SocialNetwork begin
	ReadCSV -> csv
	csv.path("/Users/jeffreyguenther/Projects/Thesis/shiro/example_code/VAST2008MC3/CellPhoneCallRecords.csv")

	UndirectedGraph -> g
	g.table(csv.table[0])
	g.columnA("From")
	g.columnB("To")

	Point -> p
	p.x(100)
	p.y(100)

	GraphView -> viewer
	viewer.g(g.g[0])
	viewer.layout("FR")
	viewer.origin(p.point[0])
	viewer.width(1000)
	viewer.height(1000)
end

state testing begin
	Graph SocialNetwork
end